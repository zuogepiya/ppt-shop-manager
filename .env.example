# 数据库配置（必填）
DATABASE_URL="postgresql://username:password@host:5432/dbname"

# 环境标识
NODE_ENV="production"

# Session 密钥（必填 - 请生成随机字符串）
SESSION_SECRET="your-random-secret-key-here-change-this"

# 应用端口
PORT=5000

